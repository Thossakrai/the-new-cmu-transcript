{
  "name": "The Transcript",
  "version": "1.1",
  "description": "The transcript is a extension to re-render CMU transcript page.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www3.reg.cmu.ac.th/transcript/result.php?time=*"],
      "js": [
        "/assets/vendor/jquery/dist/jquery.min.js",
        "/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js",
        "/assets/js/argon.min.js",
        "/assets/js/lodash.js",
        "/assets/vendor/chart.js/dist/Chart.min.js",
        "new-transcript.js"
      ],
      "css": ["/assets/css/argon.min.css", "/style.css"]
    }
  ]
}
